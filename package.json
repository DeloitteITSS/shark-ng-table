  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Quantas/shark-ng-table/issues"
  },
  "homepage": "https://github.com/Quantas/shark-ng-table#readme",
  "dependencies": {
    "@angular/common": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/router": "^5.0.0",
    "core-js": "^2.5.3",
    "rxjs": "^5.5.5"
  },
  "devDependencies": {
    "@angular/compiler": "^5.0.0",
    "@angular/compiler-cli": "^5.0.0",
    "rollup": "^0.52.3",
    "typescript": "2.4.2",
    "uglify-js": "^3.2.2"
  }
